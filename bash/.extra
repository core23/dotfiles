PATH=/opt/local/bin
PATH=/opt/local/sbin:$PATH
PATH=/bin:$PATH
PATH=/sbin:$PATH
PATH=/usr/bin:/bin:$PATH
PATH=/usr/sbin:$PATH
PATH=/usr/local/bin:$PATH
PATH=/usr/local/git/bin:$PATH
PATH=/opt/X11/bin:$PATH

PATH=$MYSQL_BIN:$PATH
PATH=~/.gems/bin:$PATH
PATH=~/.composer/vendor/bin:$PATH
PATH=~/.rvm/bin:$PATH

if [ -d ~/bin ]; then
	PATH=:~/bin:$PATH  # add your bin folder to the path, if you have it.  It's a good place to add all your scripts
fi

export PATH
